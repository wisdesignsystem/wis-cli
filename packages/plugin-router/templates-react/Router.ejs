import {
  <% if (browserHistory) { %>
  BrowserRouter,
  <% } else { %>
  HashRouter,
  <% } %>
  Routes,
  Route,
} from '@wiscore/router'

<% pages.forEach(function(page) { %>
import <%= page.pageName %> from './pages/<%= page.pageName %>'
<% }) %>
<% if (notFoundPage) { %>
import NotFound from './pages/<%= notFoundPage.pageName %>'
<% } else { %>
import NotFound from './pages/NotFound'
<% } %>

export default function Router({ basename = '/' }) {
  return (
    <<% if (browserHistory) { %>BrowserRouter<% } else { %>HashRouter<% } %>>
      <Routes>
        <Route path={basename}>
          <% pages.forEach(function(page) { %>
          <Route path="<%= page.routePath %>" element={<<%= page.pageName %> />} />
          <% }) %>
          <Route path="*" element={<NotFound />} />
        </Route>
      </Routes>
    </<% if (browserHistory) { %>BrowserRouter<% } else { %>HashRouter<% } %>>
  )
}
